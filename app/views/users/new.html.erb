<%= render 'shared/content_title', title: "Register New User" %>

<div class = "well">
  <%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
  <br>
  <br>
    <%= f.submit 'Register', class: 'btn btn-success btn primary' %>

  <% end %> 
</div>